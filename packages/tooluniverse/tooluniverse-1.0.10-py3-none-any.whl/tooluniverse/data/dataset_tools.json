[
  {
    "type": "DatasetTool",
    "name": "drugbank_vocab_search",
    "description": "Search the DrugBank vocabulary dataset for drugs by name, ID, synonyms, or other fields using text-based queries. Returns detailed drug information including DrugBank ID, common name, CAS number, UNII, and synonyms.",
    "hf_dataset_path": {
      "repo_id": "agenticx/DrugbankVocabulary",
      "path_in_repo": "drugbank_vocabulary.csv",
      "token": null,
      "save_to_local_dir": null
    },
    "parameter": {
      "type": "object",
      "properties": {
        "query": {
          "type": "string",
          "description": "Search query string. Can be drug name, synonym, DrugBank ID, or any text to search for.",
          "minLength": 1
        },
        "search_fields": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Fields to search in. Available fields: 'DrugBank ID', 'Accession Numbers', 'Common name', 'CAS', 'UNII', 'Synonyms', 'Standard InChI Key'."
        },
        "case_sensitive": {
          "type": "boolean",
          "description": "Whether the search should be case sensitive."
        },
        "exact_match": {
          "type": "boolean",
          "description": "Whether to perform exact matching instead of substring matching."
        },
        "limit": {
          "type": "integer",
          "description": "Maximum number of results to return.",
          "minimum": 1,
          "maximum": 1000
        }
      },
      "required": [
        "query",
        "search_fields",
        "case_sensitive",
        "exact_match",
        "limit"
      ]
    },
    "query_schema": {
      "search_fields": [
        "Common name",
        "Synonyms",
        "DrugBank ID"
      ],
      "case_sensitive": false,
      "exact_match": false,
      "limit": 10
    },
    "return_schema": {
      "type": "object",
      "description": "Search results with matched drug records and search metadata",
      "properties": {
        "query": {
          "type": "string",
          "description": "The original search query string"
        },
        "total_results": {
          "type": "integer",
          "description": "Total number of matching results found"
        },
        "results": {
          "type": "array",
          "description": "Array of matching drug records",
          "items": {
            "type": "object",
            "properties": {
              "DrugBank ID": {
                "type": "string",
                "description": "Drug identifier (e.g., 'DB00945')"
              },
              "Accession Numbers": {
                "type": "string",
                "description": "Alternative drug identifiers"
              },
              "Common name": {
                "type": "string",
                "description": "Drug common name (e.g., 'Acetylsalicylic acid')"
              },
              "CAS": {
                "type": "string",
                "description": "Chemical Abstracts Service number"
              },
              "UNII": {
                "type": "string",
                "description": "Unique Ingredient Identifier"
              },
              "Synonyms": {
                "type": "string",
                "description": "Alternative drug names and synonyms"
              },
              "Standard InChI Key": {
                "type": "string",
                "description": "International Chemical Identifier key"
              },
              "matched_fields": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Fields that matched the search query (e.g., ['Synonyms', 'Common name'])"
              }
            }
          }
        },
        "search_parameters": {
          "type": "object",
          "description": "Parameters used for the search",
          "properties": {
            "search_fields": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "case_sensitive": {
              "type": "boolean"
            },
            "exact_match": {
              "type": "boolean"
            },
            "limit": {
              "type": "integer"
            }
          }
        }
      }
    }
  },
  {
    "type": "DatasetTool",
    "name": "drugbank_vocab_filter",
    "description": "Filter the DrugBank vocabulary dataset based on specific field criteria. Use simple field-value pairs to filter drugs by properties like names, IDs, and chemical identifiers.",
    "hf_dataset_path": {
      "repo_id": "agenticx/DrugbankVocabulary",
      "path_in_repo": "drugbank_vocabulary.csv",
      "token": null,
      "save_to_local_dir": null
    },
    "parameter": {
      "type": "object",
      "properties": {
        "field": {
          "type": "string",
          "description": "The field to filter on",
          "enum": [
            "DrugBank ID",
            "Accession Numbers",
            "Common name",
            "CAS",
            "UNII",
            "Synonyms",
            "Standard InChI Key"
          ]
        },
        "condition": {
          "type": "string",
          "description": "The type of filtering condition to apply. Filter is case-insensitive.",
          "enum": [
            "contains",
            "starts_with",
            "ends_with",
            "exact",
            "not_empty"
          ]
        },
        "value": {
          "type": "string",
          "description": "The value to filter by. Not required when condition is 'not_empty'. Examples: 'insulin' (for contains), 'DB00' (for starts_with), 'acid' (for ends_with), 'Aspirin' (for exact)"
        },
        "limit": {
          "type": "integer",
          "description": "Maximum number of results to return.",
          "minimum": 1,
          "maximum": 1000
        }
      },
      "required": [
        "field",
        "condition",
        "limit"
      ]
    },
    "query_schema": {
      "field": "Common name",
      "condition": "contains",
      "limit": 10
    },
    "return_schema": {
      "type": "object",
      "description": "Filter results with matched drug records and filter metadata",
      "properties": {
        "total_matches": {
          "type": "integer",
          "description": "Total number of records matching the filter criteria"
        },
        "returned_results": {
          "type": "integer",
          "description": "Number of results actually returned (limited by 'limit' parameter)"
        },
        "results": {
          "type": "array",
          "description": "Array of filtered drug records",
          "items": {
            "type": "object",
            "properties": {
              "DrugBank ID": {
                "type": "string",
                "description": "Drug identifier (e.g., 'DB00001')"
              },
              "Accession Numbers": {
                "type": "string",
                "description": "Alternative drug identifiers"
              },
              "Common name": {
                "type": "string",
                "description": "Drug common name (e.g., 'Lepirudin')"
              },
              "CAS": {
                "type": "string",
                "description": "Chemical Abstracts Service number"
              },
              "UNII": {
                "type": "string",
                "description": "Unique Ingredient Identifier"
              },
              "Synonyms": {
                "type": "string",
                "description": "Alternative drug names and synonyms"
              },
              "Standard InChI Key": {
                "type": "string",
                "description": "International Chemical Identifier key"
              }
            }
          }
        },
        "applied_filter": {
          "type": "string",
          "description": "Human-readable description of the applied filter (e.g., 'Common name contains insulin')"
        },
        "filter_parameters": {
          "type": "object",
          "description": "Parameters used for filtering",
          "properties": {
            "field": {
              "type": "string"
            },
            "condition": {
              "type": "string"
            },
            "value": {
              "type": [
                "string",
                "null"
              ]
            },
            "limit": {
              "type": "integer"
            }
          }
        }
      }
    }
  },
