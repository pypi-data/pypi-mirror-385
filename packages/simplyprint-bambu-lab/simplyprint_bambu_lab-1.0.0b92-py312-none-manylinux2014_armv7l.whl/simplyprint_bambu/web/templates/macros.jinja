{% macro tooltipHelp(text, icon) %}
    {% set icon = icon|default(faQuestionCircle()) %}
    <span class="d-inline-block text-info" tabindex="0" data-bs-toggle="tooltip" title="{{ text }}">
    {{ icon }}
</span>
{% endmacro %}

{% macro backBtn(url, confirmation, onclick) %}
    <div>
        <a href="{{ url }}"

                {% if confirmation %}
           hx-confirm="{{ confirmation }}"
                {% endif %}

                {% if onclick %}
           onclick="{{ onclick }}"
                {% endif %}

           draggable="false"
           class="btn btn-secondary btn-sm me-2">
            {{ faBackArrow() }}
        </a>
    </div>
{% endmacro %}

{% macro accordian(prefix, hide) %}
    <div class="accordion mb-3 {{ 'd-none' if hide else '' }}" id="{{ prefix }}">
        {{ caller() }}
    </div>
{% endmacro %}

{% macro accordianItem(prefix, name, category, title) %}
    <div class="accordion-item">
        <h2 class="accordion-header" id="{{ prefix }}-{{ name }}">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                    data-bs-target="#{{ prefix }}-{{ name }}-collapse" aria-expanded="false"
                    aria-controls="{{ prefix }}-{{ name }}-collapse">
                <span>
                    {% if category %}
                        <strong>{{ category }}:</strong>
                    {% endif %}
                    {{ title }}
                </span>
            </button>
        </h2>
        <div id="{{ prefix }}-{{ name }}-collapse" class="accordion-collapse collapse"
             aria-labelledby="{{ prefix }}-{{ name }}" data-bs-parent="#{{ prefix }}">
            <div class="accordion-body">
                {{ caller() }}
            </div>
        </div>
    </div>
{% endmacro %}


