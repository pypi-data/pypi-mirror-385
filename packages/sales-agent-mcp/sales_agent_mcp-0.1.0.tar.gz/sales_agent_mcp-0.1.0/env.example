# Sales Agent System Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI Configuration (Required)
OPENAI_API_KEY=your-openai-api-key-here

# SendGrid Configuration (Optional - for actual email sending)
SENDGRID_API_KEY=your-sendgrid-api-key-here
SENDER_EMAIL=verified@yourdomain.com
RECIPIENT_EMAILS=prospect1@example.com,prospect2@example.com

# Optional: LangSmith for tracing (if using monitoring)
LANGCHAIN_API_KEY=your-langsmith-api-key-here

# Debug Mode (Optional)
DEBUG=false
