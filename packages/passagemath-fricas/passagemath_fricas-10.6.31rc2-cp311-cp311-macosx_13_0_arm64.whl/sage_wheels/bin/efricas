#!/bin/sh -
FRICASEDITOR='emacsclient +$line $name >/dev/null 2>&1'
export FRICASEDITOR
FRICASCMD='/Users/runner/sage-local/lib/fricas/target/aarch64-apple-darwin23.6.0/bin/fricas'
export FRICASCMD
if which emacs; then
  emacs --eval '(progn (server-start) (setq load-path (cons (quote "/Users/runner/sage-local/lib/fricas/emacs") load-path)) (setq auto-mode-alist (cons (quote ("\.fri$" . fricas-mode)) auto-mode-alist)) (require (quote fricas)))' -f fricas $@;
else 
  echo You must have installed emacs to run efricas.;
fi
