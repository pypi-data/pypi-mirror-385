# Supabase Configuration
# Get these values from your Supabase project settings:
# https://app.supabase.com/project/_/settings/api

# Your Supabase project URL
SUPABASE_URL=https://yourproject.supabase.co

# Your Supabase service role key (or anon key for public buckets)
# WARNING: Keep this key secure and never commit it to version control
SUPABASE_KEY=your-service-role-key-here

# Optional: Default bucket name for operations
DEFAULT_BUCKET=your-bucket-name

# Optional: Logging configuration
LOG_LEVEL=INFO
