{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"verbatimModuleSyntax": true,
		"module": "ESNext",		
    	"moduleResolution": "bundler",
		"target": "es2022",
    	"lib": ["es2022", "dom"], 
	},
	"exclude": ["node_modules", "dist", "./gradio.config.js"]
}
