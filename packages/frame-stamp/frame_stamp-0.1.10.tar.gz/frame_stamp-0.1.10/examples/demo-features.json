{"templates":[
  {
    "defaults": {
        "font_size": "2.7u",
        "logo_img": "base64::MOOOV_logo.png::",
        "label_font_size": "2.5p",
        "panel_color": [0,0,0,0],
        "label_color": [255,255,255,200],
        "text_color": [255,255,255,255],
        "text_spacing": 10,
        "text_padding": "5u",
        "backdrop_offset": 3,
        "backdrop_color": [0,0,0,64],

        "debug": {"enabled": 1, "color":  "yellow", "canvas_bound": 1, "canvas_bound_color": "red",
        "parent": true}
      },
      "variables": {
        "image": "/home/paul/Desktop/tests/images/download.jpeg",
        "_timeline": {"start": 0, "end": 180}
      },
      "shapes": [
        {
          "type": "grid", "w": "parent.w", "h": "=parent.h/6", "columns": 9, 
          "id": "grid1",
          "shapes":[
            {
              "type": "label", "text": "top",
              "align_v": "top", "align_h": "center",
              "font_size": "$label_font_size"
            },
            {
              "type": "label", "text": "left", "font_size": "$label_font_size",
              "align_v": "center", "align_h": "left"
            },
            {
              "type": "label", "text": "bottom", "font_size": "$label_font_size",
              "align_v": "bottom", "align_h": "center"
            },
            {
              "type": "label", "text": "right", "font_size": "$label_font_size",
              "align_v": "center", "align_h": "right"
            },
            
            {
              "type": "label", "text": "left-top", "font_size": "$label_font_size",
              "align_v": "top", "align_h": "left"
            },
            {
              "type": "label", "text": "right-top", "font_size": "$label_font_size",
              "align_v": "top", "align_h": "right"
            },
            {
              "type": "label", "text": "left-bot", "font_size": "$label_font_size",
              "align_v": "bottom", "align_h": "left"
            },
            {
              "type": "label", "text": "right-bot", "font_size": "$label_font_size",
              "align_v": "bottom", "align_h": "right"
            },
            {
              "type": "label", "text": "center", "font_size": "$label_font_size",
              "align_v": "center", "align_h": "center"
            } 
          ]
        },
        {
          "type": "rect", "id": "panel",
          "w": "=parent.w*0.8", "h": "7p",
          "align_h": "center", "y": "=grid1.h+1p",
          "color": [0,0,0,100]
        },
        {
          "type": "grid", "columns": 4, "parent": "panel",
          "w": "parent.w", "h": "parent.h",
          "shapes": [
            {
              "type": "label", "text": "Project:",
              "align_v": "center", "align_h": "right",
              "padding_right": "1p"
            },
            {
              "type": "label", "text": "PROJECT NAME",
              "align_v": "center", "align_h": "left",
              "padding_left": "1p"
            },
            {
              "type": "label", "text": "Asset:",
              "align_v": "center", "align_h": "right",
              "padding_right": "1p"
            },
            {
              "type": "label", "text": "CAT",
              "align_v": "center", "align_h": "left",
              "padding_left": "1p"
            }
            
          ]
        },

        {
          "type": "label", "text": "DEMOREEL",
          "font_size": "10p", "align_v": "center", "align_h": "center",
          "text_color": [0,0,0,0], "outline": {"width": 2, "hardness": 10, "color": [0, 255, 0, 255]},
          "rotate": "`=math.sin($timeline_value*0.05)*40`", "id": "demo"
        },
        {
          "type": "line", "parent": "demo",
          "y": "=parent.h*1.1",
          "points":[
            [0, 0],
            ["parent.w",0]
          ],
          "color": [0, 255, 0, 255]
        },
        {
          "type": "rect", "w": 50, "h": 10, "parent": "demo", "id": "box2",
          "y": 100, "color": "red",
          "x": "`=(math.sin($timeline_value*0.1)*parent.w)/2 + parent.w*0.4 + self.w/2`"
        },
      
        {
          "type": "rect", "id": "box1", "rotation_pivot": ["=self.x+(self.w/2)", "self.y"],
          "w": 10, "h": 100, "rotate": "`=math.sin($timeline_value*0.1)*40`",
          "x": "=parent.w*0.9", "y": 500,
          "color": "blue"
        },
        {
          "type": "rect", "w": 20, "h": 20, "parent": "box1",
          "x": -5, "y": "parent.h"
        },
   
        {
          "type": "circle", "w": 100, "h": 100, "id": "circ",
          "color": "white", "outline": {"color": "red", "width": 3},
          "y": "=parent.h-self.h", 
          "rotate": "`=-math.degrees(($timeline_value*4) / self.radius)`",
          "x": "=$timeline_value*4"
        },
        {
          "type": "label", "text": "GO>", "text_color": "red", "parent": "circ", "font_size": "4p",
          "align_v": "center", "align_h": "center"
        },
        {
          "type": "label", "text": ">", "text_color": "orange", "font_size": "8p",
          "x": "=circ.x+circ.w+self.w/4", "align_v": "bottom"
          , "y": "=-circ.w/2+(self.w/2)"
        }
        
      ]
}

]}
