{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "android": {
      "minSdkVersion": 24
    },
    "icon": [
      "icons/icon.ico",
      "icons/icon.png"
    ]
  },
  "productName": "Neuro Simulator",
  "mainBinaryName": "neuro",
  "version": "0.2.16",
  "identifier": "cafe.jiahui.neuro",
  "plugins": {
    "store": null
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": "neuro.exe",
        "width": 1280,
        "minWidth": 400,
        "minHeight": 600,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
