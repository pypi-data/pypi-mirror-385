{% set title = gettext("Musical chairs") %}
{% set back_url = "/" %}
{% set script = "games_chairs" %}
{% set styles = ["games_chairs"] %}
{% extends "base_static.jinja2" %}

{% block content %}

<h3 id="text-choose">{% trans %}Choose a track{% endtrans %}</h3>
<h3 hidden id="text-start">{% trans %}Press any key or click to start{% endtrans %}</h3>
<div hidden class="icon icon-loading spinning" id="spinner"></div>

<div class="boxes" id="boxes">
</div>

<audio id="audio"></audio>

{% endblock content %}
