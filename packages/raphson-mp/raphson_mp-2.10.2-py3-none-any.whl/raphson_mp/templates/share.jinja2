{% set title = gettext("Share") %}
{% set styles = ["share"] %}
{% extends "base_static.jinja2" %}

{% block content %}

<p>{% trans %}{{ shared_by }} has shared these tracks with you:{% endtrans %}</p>

<ul>
    {% for track_code, display in tracks %}
        <li><a href="/share/{{ share_code }}/{{ track_code }}">{{ display }}</a></li>
    {% endfor %}
</ul>

{% endblock content %}
