# Error Messages
msgid "No workflow attached to {obj_label}({obj_pk}). Use attach_workflow_to_object() to attach a workflow first."
msgstr "No workflow attached to {obj_label}({obj_pk}). Use attach_workflow_to_object() to attach a workflow first."

msgid "Workflow is not in progress (status: {status}). Current status must be 'in_progress' to perform this action."
msgstr "Workflow is not in progress (status: {status}). Current status must be 'in_progress' to perform this action."

msgid "Workflow '{workflow_name}' has no pipelines. Add at least one pipeline with stages to use this workflow."
msgstr "Workflow '{workflow_name}' has no pipelines. Add at least one pipeline with stages to use this workflow."

msgid "Pipeline '{pipeline_name}' has no stages. Add at least one stage with approvals to use this pipeline."
msgstr "Pipeline '{pipeline_name}' has no stages. Add at least one stage with approvals to use this pipeline."

msgid "No next stage found, completing workflow for {obj_label}({obj_pk})"
msgstr "No next stage found, completing workflow for {obj_label}({obj_pk})"

msgid "No current pipeline found for attachment {attachment_id}, cannot determine next stage"
msgstr "No current pipeline found for attachment {attachment_id}, cannot determine next stage"

msgid "No user found for building approval steps for {obj_label}({obj_pk}), this may cause issues with approval flow"
msgstr "No user found for building approval steps for {obj_label}({obj_pk}), this may cause issues with approval flow"

msgid "Workflow already started (status: {status}). Cannot start a workflow that is already in progress."
msgstr "Workflow already started (status: {status}). Cannot start a workflow that is already in progress."

msgid "Workflow '{workflow_name}' is not active and cannot be attached. Please activate the workflow before attaching it to objects."
msgstr "Workflow '{workflow_name}' is not active and cannot be attached. Please activate the workflow before attaching it to objects."

