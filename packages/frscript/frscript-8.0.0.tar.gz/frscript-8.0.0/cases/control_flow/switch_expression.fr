// !result: 5

void main() {
    int x = 2
    int y = 3
    
    switch (x + y) {
        case 5:
            println("result: 5")
        case 6:
            println("result: 6")
        case 7:
            println("result: 7")
        case 8:
            println("result: 8")
        default:
            println("result: other")
    }
}
