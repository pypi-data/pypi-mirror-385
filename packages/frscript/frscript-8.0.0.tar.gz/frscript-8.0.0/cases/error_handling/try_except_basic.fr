// !Before error\nCaught division by zero!\nProgram continues

int main() {
    try {
        println("Before error")
        float x = 10.0 / 0.0
        println("After error (should not print)")
    } except "ZeroDivisionError" {
        println("Caught division by zero!")
    }
    
    println("Program continues")
    return 0
}
