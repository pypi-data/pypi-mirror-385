    "/v3/messages/chats/{chat_id}": {
      "get": {
        "tags": [
          "v3",
          "message",
          "OpenAPI v1"
        ],
        "summary": "Get Messages",
        "operationId": "get_messages_v3_messages_chats__chat_id__get",
        "parameters": [
          {
            "name": "chat_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Chat Id"
            }
          },
          {
            "name": "message_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "title": "Message Id"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 20,
              "title": "Limit"
            }
          },
          {
            "name": "order",
            "in": "query",
            "required": false,
            "schema": {
              "enum": [
                "desc",
                "asc"
              ],
              "type": "string",
              "description": "sort(desc or asc) messages based on their ids.",
              "default": "desc",
              "title": "Order"
            },
            "description": "sort(desc or asc) messages based on their ids."
          },
          {
            "name": "cmp",
            "in": "query",
            "required": false,
            "schema": {
              "enum": [
                "lte",
                "lt",
                "gte",
                "gt",
                "bt"
              ],
              "type": "string",
              "description": "Comparison operator to use with `message_id` for pagination. Use '<'( or '<=') to get oldermessages and '>'(or '>=') to get newer ones.",
              "default": "lt",
              "title": "Cmp"
            },
            "description": "Comparison operator to use with `message_id` for pagination. Use '<'( or '<=') to get oldermessages and '>'(or '>=') to get newer ones."
          }
        ],
