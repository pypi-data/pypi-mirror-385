{
  "name": "@amzn/sagemaker-connection-magics-jlextension",
  "version": "0.1.0",
  "description": "SageMaker module for Brazil package SageMakerConnectionMagicsJLExtension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://code.amazon.com/packages/SageMakerConnectionMagicsJLExtension/",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://code.amazon.com/packages/SageMakerConnectionMagicsJLExtension/.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib:prod && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "clean": "npm run clean:lib",
    "clean:lib": "rm -rf lib tsconfig.tsbuildinfo",
    "clean:dist": "rm -rf dist",
    "clean:lintcache": "rm -rf .eslintcache .stylelintcache",
    "clean:labextension": "rm -rf sagemaker_connection_magics_jlextension/labextension sagemaker_connection_magics_jlextension/_version.py",
    "clean:deps": "rm -rf node_modules",
    "clean:all": "npm run clean:lib && npm run clean:labextension && npm run clean:lintcache && npm run clean:dist && npm run clean:deps",
    "eslint": "npm run eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "npm run build",
    "lint": "npm run stylelint && npm run prettier && npm run eslint",
    "lint:check": "npm run stylelint:check && npm run prettier:check && npm run eslint:check",
    "prepublishOnly": "run-s clean build:lib:prod",
    "prettier": "npm run prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "npm run prettier:base --check",
    "check:format": "prettier --check ./src",
    "fix:format": "prettier --write ./src",
    "stylelint": "npm run stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "release": "run-s clean build:lib:prod",
    "start:server": "npm run generate:creds && npx json-server config.json --routes routes.json",
    "watch:labextension": "jupyter labextension watch .",
    "generate:creds:ada": "ada credentials update --once --account $npm_package_config_accountId --role $npm_package_config_role",
    "generate:creds:file": "tsx build-scripts/generate-local-creds.ts",
    "generate:creds": "run-s generate:creds:ada generate:creds:file",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/cells": "^4.0.10",
    "@jupyterlab/codeeditor": "^4.0.10",
    "@jupyterlab/codemirror": "^4.0.0",
    "@jupyterlab/docmanager": "^4.0.10",
    "@jupyterlab/launcher": "^4.0.10",
    "@jupyterlab/notebook": "^4.0.10",
    "@jupyterlab/terminal": "^4.0.10",
    "@jupyterlab/completer": "^4.0.10",
    "@jupyterlab/ui-components": "^4.0.0",
    "@jupyterlab/rendermime": "^4.0.10",
    "@jupyterlab/rendermime-interfaces": "^3.10.7",
    "@rjsf/validator-ajv8": "^5.13.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-md5": "^1.3.1",
    "@aws-sdk/types": "^3.600.0",
    "@types/jquery": "^3.5.1",
    "aws-sdk": "^2.1648.0",
    "jquery": "^3.5.1",
    "yarn": "^1.22.22",
    "duplicate-package-checker-webpack-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@jupyterlab/builder": "^4.0.0",
    "@jupyterlab/logconsole": "^4.0.10",
    "@jupyterlab/testutils": "^4.0.10",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@types/fs-extra": "^11.0.3",
    "@types/jest": "^29.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/node": "~20.3.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "aws-sdk": "^2.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@lumino/coreutils": "^1.11.0",
    "@lumino/widgets": "^1.37.2"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "sagemaker_connection_magics_jlextension/labextension",
    "schemaDir": "schema"
  },
  "npm-pretty-much": {
    "runTest": "never",
    "ciBuild": "always"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts",
    "tests",
    "**/__tests__",
    "ui-tests"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}
