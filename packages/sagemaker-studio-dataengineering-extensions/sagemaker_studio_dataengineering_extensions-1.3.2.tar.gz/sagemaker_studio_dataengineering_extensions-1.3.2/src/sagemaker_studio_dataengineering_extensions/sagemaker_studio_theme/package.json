{
  "name": "@amzn/sagemaker-ui-theme-jlplugin",
  "version": "0.9.0",
  "description": "Amazon SageMaker Unified Studio color theme for Jupyter lab extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://code.amazon.com/packages/MaxDomeThemeJLPlugin/",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://code.amazon.com/packages/MaxDomeThemeJLPlugin/.git"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:prod": "npm run clean && npm run build:lib:prod && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "clean": "npm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:dist": "rimraf dist",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf sagemaker_ui_theme/labextension sagemaker_ui_theme/_version.py",
    "clean:deps": "rimraf node_modules",
    "clean:all": "npm run clean:lib && npm run clean:labextension && npm run clean:lintcache && npm run clean:dist && npm run clean:deps",
    "eslint": "npm run eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "npm run build",
    "lint": "npm run stylelint && npm run prettier && npm run eslint",
    "lint:check": "npm run stylelint:check && npm run prettier:check && npm run eslint:check",
    "prepublishOnly": "run-s clean build:lib:prod",
    "prettier": "npm run prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "npm run prettier:base --check",
    "stylelint": "npm run stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "release": "run-s clean build:lib:prod",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/ui-components": "^4.0.0",
    "react-dom": "^18.2.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/fs-extra": "^11.0.3",
    "@types/node": "~18.13.0",
    "aws-sdk": "^2.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint": "^14.3.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@lumino/coreutils": "^1.11.0",
    "@lumino/widgets": "^1.37.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "sagemaker_ui_theme/labextension",
    "themePath": "style/index.css"
  },
  "npm-pretty-much": {
    "runTest": "never",
    "ciBuild": "always"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}
