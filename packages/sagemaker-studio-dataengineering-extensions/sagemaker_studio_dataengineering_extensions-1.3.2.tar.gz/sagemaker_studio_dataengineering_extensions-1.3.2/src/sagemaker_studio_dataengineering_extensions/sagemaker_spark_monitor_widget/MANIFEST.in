recursive-include sagemaker_sparkmonitor *.jar

include LICENSE
include README.md
include pyproject.toml

include package.json
include install.json
include yarn.lock

graft sagemaker_sparkmonitor/labextension
graft sagemaker_sparkmonitor/static
graft jupyter-config

