            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path schema1:object ],
        [ sh:datatype xsd:string ;
            sh:description "The identifier of the category of the part in the template" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path opencloning_linkml:category_id ],
        [ sh:datatype xsd:string ;
            sh:description "Used to specify the name of the output sequence" ;
