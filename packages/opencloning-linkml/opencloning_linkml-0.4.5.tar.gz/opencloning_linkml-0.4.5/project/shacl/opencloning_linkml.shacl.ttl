            sh:path opencloning_linkml:sources ],
        [ sh:description "The version of the frontend that was used to generate this cloning strategy" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path opencloning_linkml:frontend_version ],
        [ sh:class bioschemas:DNA ;
            sh:description "The sequences that are used in the cloning strategy" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path opencloning_linkml:sequences ],
        [ sh:class opencloning_linkml:Primer ;
            sh:description "The primers that are used in the cloning strategy" ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path opencloning_linkml:primers ] ;
    sh:targetClass opencloning_linkml:CloningStrategy .

