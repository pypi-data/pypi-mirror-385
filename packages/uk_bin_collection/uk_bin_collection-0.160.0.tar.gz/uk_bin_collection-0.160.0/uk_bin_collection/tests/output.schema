{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/BinData",
    "definitions": {
        "BinData": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "bins": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Bin"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "bins"
            ],
            "title": "BinData"
        },
        "Bin": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "type": {
                    "type": "string"
                },
                "collectionDate": {
                    "type": "string",
                    "pattern": "\\d{2}/\\d{2}/\\d{4}"
                }
            },
            "required": [
                "collectionDate",
                "type"
            ],
            "title": "Bin"
        }
    }
}
