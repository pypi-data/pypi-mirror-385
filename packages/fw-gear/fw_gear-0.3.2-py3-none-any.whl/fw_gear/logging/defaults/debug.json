{
  "disable_existing_loggers": false,
  "filters": {
    "err_filter": {
      "()": "PlaceHolder",
      "param": "noshow"
    },
    "out_filter": {
      "()": "PlaceHolder",
      "param": "noshow"
    }
  },
  "formatters": {
    "gear": {
      "datefmt": "%Y%m%d %H:%M:%S",
      "format": "[{asctime:3s}   {levelname:8s}  {processName:15s}  {name:20s}:{lineno}]  {message}",
      "style": "{"
    }
  },
  "handlers": {
    "gear": {
      "class": "logging.StreamHandler",
      "filters": [
        "out_filter"
      ],
      "formatter": "gear",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    },
    "gear_err": {
      "class": "logging.StreamHandler",
      "filters": [
        "err_filter"
      ],
      "formatter": "gear",
      "level": "DEBUG",
      "stream": "ext://sys.stderr"
    }
  },
  "loggers": {
    "": {
      "handlers": [
        "gear",
        "gear_err"
      ],
      "level": "DEBUG"
    }
  },
  "version": 1
}
