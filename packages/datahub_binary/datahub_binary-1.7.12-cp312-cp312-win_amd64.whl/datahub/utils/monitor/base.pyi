from dataclasses import dataclass

@dataclass
class Monitor:
    def to(self, who: str): ...
    def at(self, who: str): ...
    def clear(self) -> None: ...
    def info(self, msg: str): ...
    def error(self, msg: str): ...
    def warning(self, msg: str): ...
    def debug(self, msg: str): ...
