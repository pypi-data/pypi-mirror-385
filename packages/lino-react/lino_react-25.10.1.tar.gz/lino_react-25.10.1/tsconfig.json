{
    "compilerOptions": {
        "outDir": "./lino_react/react/static/react/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es2019",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "typeRoots": [
            "node_modules/@types",
            "./lino_react/react/components/defs"
        ],
    },
    "exclude": ["electron/"],
}
