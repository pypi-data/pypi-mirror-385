<tryton>
    <data grouped="1">
        <record model="ir.message" id="msg_dav_status">
            <field name="text">Connection Status: "%(status)s".</field>
        </record>
        <record model="ir.message" id="msg_dav_success">
            <field name="text">Connection succesful (DAV OK).</field>
        </record>
        <record model="ir.message" id="msg_dav_401">
            <field name="text">Wrong credentials or no DAV access.</field>
        </record>
        <record model="ir.message" id="msg_dav_unexpected">
            <field name="text">Unexpected Status %(status_code)s: "%(reason)s".</field>
        </record>
        <record model="ir.message" id="msg_url_unique_per_user">
            <field name="text">Only unique URLs are allowed for this user.</field>
        </record>
    </data>
</tryton>
