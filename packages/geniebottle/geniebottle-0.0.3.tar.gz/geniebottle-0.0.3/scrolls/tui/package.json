		"marked": "^15.0.12",
		"marked-terminal": "^7.3.0",
		"meow": "^11.0.0",
		"react": "^19.2.0",
		"sharp": "^0.34.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/preset-react": "^7.18.6",
		"@vdemedes/prettier-config": "^2.0.1",
		"chalk": "^5.6.2",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"import-jsx": "^5.0.0",
		"prettier": "^2.8.7",
		"xo": "^0.53.1"
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
