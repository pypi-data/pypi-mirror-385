{
  "name": "mcp-active-cell-bridge",
  "version": "1.0.0",
  "description": "JupyterLab extension to bridge active cell content to MCP server",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/caidish/instrMCP",
  "license": "MIT",
  "author": {
    "name": "Jiaqi Cai",
    "email": "jiaqic@mit.edu"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/caidish/instrMCP.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension && jlpm postbuild",
    "build:labextension": "jupyter labextension build .",
    "build:lib": "tsc --sourceMap",
    "postbuild": "rm -f mcp_active_cell_bridge/labextension/static/*.js && cp static/static/*.js static/static/*.json mcp_active_cell_bridge/labextension/static/ && cp static/package.json mcp_active_cell_bridge/labextension/package.json",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.2.0",
    "@jupyterlab/apputils": "^4.2.0",
    "@jupyterlab/cells": "^4.2.0",
    "@jupyterlab/notebook": "^4.2.0",
    "@jupyterlab/services": "^7.2.0",
    "diff": "^8.0.2"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.2.0",
    "@types/diff": "^8.0.0",
    "rimraf": "^5.0.1",
    "typescript": "~5.1.6"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
