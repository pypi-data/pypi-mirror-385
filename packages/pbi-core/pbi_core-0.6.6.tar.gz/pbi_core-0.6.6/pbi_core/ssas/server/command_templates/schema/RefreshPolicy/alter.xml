<RefreshPolicy>                                
  <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sql="urn:schemas-microsoft-com:xml-sql">
    <xs:element>
      <xs:complexType>
        <xs:sequence>
          <xs:element type="row"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:complexType name="row">
      <xs:sequence>
        <xs:element minOccurs="0" name="ID" sql:field="ID" type="xs:unsignedLong"/>
        <xs:element minOccurs="0" name="ID.Table" sql:field="ID.Table" type="xs:string"/>
        <xs:element minOccurs="0" name="PolicyType" sql:field="PolicyType" type="xs:long"/>
        <xs:element minOccurs="0" name="RollingWindowGranularity" sql:field="RollingWindowGranularity" type="xs:long"/>
        <xs:element minOccurs="0" name="RollingWindowPeriods" sql:field="RollingWindowPeriods" type="xs:int"/>
        <xs:element minOccurs="0" name="IncrementalGranularity" sql:field="IncrementalGranularity" type="xs:long"/>
        <xs:element minOccurs="0" name="IncrementalPeriods" sql:field="IncrementalPeriods" type="xs:int"/>
        <xs:element minOccurs="0" name="IncrementalPeriodsOffset" sql:field="IncrementalPeriodsOffset" type="xs:int"/>
        <xs:element minOccurs="0" name="PollingExpression" sql:field="PollingExpression" type="xs:string"/>
        <xs:element minOccurs="0" name="SourceExpression" sql:field="SourceExpression" type="xs:string"/>
        <xs:element minOccurs="0" name="Mode" sql:field="Mode" type="xs:long"/>
      </xs:sequence>
    </xs:complexType>
  </xs:schema>
  {{rows}}
</RefreshPolicy>