{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "awslabs.core-mcp-server": {
      "command": "uvx",
      "args": ["awslabs.core-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "browser": {
      "command": "npx",
      "args": ["@agent-infra/mcp-server-browser@latest"]
    },
    "awslabs.aws-documentation-mcp-server": {
      "command": "uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "awslabs.cdk-mcp-server": {
      "command": "uvx",
      "args": ["awslabs.cdk-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    }
  },
  "toolsSettings": {
    "fs_write": {
      "allowedPaths": ["./src/**", "./tests/**", "./docs/**"]
    },
    "fs_read": {
      "allowedPaths": ["./src/**", "./tests/**", "./docs/**", "./README.md"]
    },
    "execute_bash": {
      "allowedCommands": [
        "git status",
        "git log --oneline -10",
        "git diff",
        "npm test",
        "python -m pytest",
        "cargo test",
        "mvn test"
      ],
      "allowReadOnly": true
    }
  },
  "allowedTools": [
    "fs_read",
    "fs_write", 
    "execute_bash",
    "thinking",
    "todo_list"
  ]
}
