    "@gradio/statustracker": "0.10.15",
    "@gradio/upload": "0.16.11",
    "@gradio/utils": "0.10.2",
    "@gradio/wasm": "0.18.1",
    "cropperjs": "^1.5.12",
    "exifr": "^7.1.3",
    "lazy-brush": "^1.0.1",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@gradio/preview": "0.14.0"
  },
  "main_changeset": true,
