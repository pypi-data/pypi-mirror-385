{
  "name": "reproducibility/v1",
  "description": "Basic reproducibility requirements",
  "required_fields": [
    "id",
    "owner",
    "pipeline",
    "outputs",
    "environment",
    "git"
  ],
  "artifacts": {
    "min_count": 1,
    "allowed_types": [
      "data",
      "model",
      "code",
      "documentation"
    ]
  },
  "environment": {
    "min_python_version": "3.8"
  },
  "git": {
    "require_clean": false
  },
  "warnings": [
    {
      "field": "signature",
      "value": "UNSIGNED-DEV",
      "message": "Receipt is not cryptographically signed"
    }
  ]
}