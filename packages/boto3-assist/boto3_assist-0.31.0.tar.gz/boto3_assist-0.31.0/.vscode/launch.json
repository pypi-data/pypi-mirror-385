    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                
                "ENVRIONMENT": "dev",
                "DEBUG_MODE": "True",
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/src:${workspaceFolder}/devops:${workspaceFolder}/tests"                                           
            }
        },
        {
            "name": "Debug DynamoDB Example Docker .env.docker",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/dynamodb/main.py",
            "console": "integratedTerminal",
            "env": {
                "LOG_DYNAMODB_ITEM_SIZE": "True",
                "ENVRIONMENT_FILE": ".env.docker",
                "ENVRIONMENT": "dev",
                "DEBUG_MODE": "True",
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/src"                                           
            }
        },
        {
            "name": "Debug DynamoDB Example Docker NoSQL Workbench",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/dynamodb/main.py",
            "console": "integratedTerminal",
            "env": {
                "LOG_DYNAMODB_ITEM_SIZE": "True",
                "ENVRIONMENT_FILE": ".env.docker.nosql.workbench",
                "ENVRIONMENT": "dev",
                "DEBUG_MODE": "True",
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/src"                                           
            }
        },
        {
            "name": "Debug DynamoDB Example AWS Profile .env.development",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/dynamodb/main.py",
            "console": "integratedTerminal",
            "env": {
                "ENVRIONMENT_FILE": ".env.development",
                "ENVRIONMENT": "dev",
                "DEBUG_MODE": "True",
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/src"                                           
            }
        },

        
        {
            "name": "Install Requirements",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/devops/install_requirements.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Build & Deploy Artifacts",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/devops/build.py",
            "console": "integratedTerminal"
        },
        
    ]
}