{% macro Z8_Theme_Toggle() -%}
<z8-theme-toggle></z8-theme-toggle>
{%- endmacro %}

{% macro Z8_Copy_Button(text, label, copiedLabel) -%}
<z8-copy-button text= "{{ text }}" label= "{{ label }}" copiedLabel= "{{ copiedLabel }}" ></z8-copy-button>
{%- endmacro %}

{% macro Z8_Online_Badge() -%}
<z8-online-badge></z8-online-badge>
{%- endmacro %}

{% macro Z8_Clock() -%}
<z8-clock></z8-clock>
{%- endmacro %}

{% macro Z8_Ping(endpoint) -%}
    <z8-ping endpoint="{{ endpoint }}"></z8-ping>
{%- endmacro %}

{% macro Z8_Island_Pref() -%}
<z8-island-pref></z8-island-pref>
{%- endmacro %}
