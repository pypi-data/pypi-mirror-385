boolean Rain
variable Sprinkler {On, Off}
variable GrassWet {Yes, No}

Rain { P(True) = 0.2 }

Sprinkler | Rain {
    P(On | True) = 0.01
    P(On | False) = 0.4
}

GrassWet | Rain, Sprinkler {
    P(Yes | True, On) = 0.99
    P(Yes | True, Off) = 0.8
    P(Yes | False, On) = 0.9
    P(Yes | False, Off) = 0.1
}
