{
  "name": "agentcoreMcpProxy",
  "type": "stdio",
  "command": "uvx",
  "args": ["mcp-agentcore-proxy"],
  "env": {
    "AGENTCORE_AGENT_ARN": "${input:agentArn}",
    "AGENTCORE_ASSUME_ROLE_ARN": "${input:assumeRoleArn}"
  },
  "inputs": [
    {
      "type": "promptString",
      "id": "agentArn",
      "description": "Enter the required AGENTCORE_AGENT_ARN",
      "password": false
    },
    {
      "type": "promptString",
      "id": "assumeRoleArn",
      "description": "Optional AGENTCORE_ASSUME_ROLE_ARN (press Enter to skip)",
      "password": false
    }
  ]
}