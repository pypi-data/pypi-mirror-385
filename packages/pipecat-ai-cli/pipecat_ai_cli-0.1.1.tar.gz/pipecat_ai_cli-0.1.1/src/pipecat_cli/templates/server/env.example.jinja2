AZURE_SPEECH_REGION=your_azure_region_here
{% if tts_service == 'azure_tts' %}
AZURE_SPEECH_VOICE_ID=your_azure_voice_id_here
{% endif %}
{% endif %}
{% if llm_service == 'azure_llm' %}
