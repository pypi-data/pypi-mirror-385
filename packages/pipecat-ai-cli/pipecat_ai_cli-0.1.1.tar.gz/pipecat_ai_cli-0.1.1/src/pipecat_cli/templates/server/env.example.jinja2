AZURE_CHATGPT_ENDPOINT=your_azure_chatgpt_endpoint_here
AZURE_CHATGPT_MODEL=your_azure_chatgpt_model_here
{% endif %}
{% if realtime_service == 'azure_realtime' %}
AZURE_REALTIME_API_KEY=your_azure_realtime_api_key_here
AZURE_REALTIME_BASE_URL=your_azure_realtime_base_url_here
AZURE_INSTRUCTIONS=  # Optional: Custom system instructions
{% endif %}

{% endif %}
{% if stt_service == 'cartesia_stt' or tts_service == 'cartesia_tts' %}
