{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://assertlang.dev/schemas/tools/timing.v1.json",
  "title": "Timing Tool",
  "type": "object",
  "properties": {
    "op": {
      "type": "string",
      "enum": [
        "sleep"
      ]
    },
    "ms": {
      "type": "integer",
      "minimum": 0
    },
    "lang": {
      "type": "string",
      "enum": [
        "python",
        "node",
        "nextjs",
        "go",
        "rust",
        "java",
        "dotnet",
        "cpp"
      ]
    }
  },
  "required": [
    "op",
    "ms"
  ],
  "additionalProperties": false
}


