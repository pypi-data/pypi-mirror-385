<resource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://datacite.org/schema/kernel-4" xsi:schemaLocation="http://datacite.org/schema/kernel-4 http://schema.datacite.org/meta/kernel-4.3/metadata.xsd">
    <identifier identifierType="DOI">{{ doi }}</identifier>
    <creators>
        {% for creator in creators %}
        <creator>
            <creatorName nameType="Personal">{{ creator.name }}</creatorName>
            <givenName>COMPLETE!</givenName>
            <familyName>COMPLETE!</familyName>
            <nameIdentifier nameIdentifierScheme="ORCID">COMPLETE!</nameIdentifier>
            <!-- <affiliation>GFZ German Research Centre for Geosciences, Potsdam, Germany</affiliation> -->
        </creator>
        {% endfor %}
    </creators>
    <titles>
        <title>{{ title }}</title>
    </titles>
    <publisher>COMPLETE PUBLISHER!</publisher>
    <publicationYear>{{ pubyear }}</publicationYear>
    <subjects>
        <subject subjectScheme="SeisData" schemeURI="https://geofon.gfz-potsdam.de/cv/seisdata" xml:lang="en">Array</subject>
        <subject subjectScheme="SeisData" schemeURI="https://geofon.gfz-potsdam.de/cv/seisdata" xml:lang="en">Temporary</subject>
        <subject subjectScheme="SeisData" schemeURI="https://geofon.gfz-potsdam.de/cv/seisdata" xml:lang="en">Strain</subject>
        <subject subjectScheme="SeisData" schemeURI="https://geofon.gfz-potsdam.de/cv/seisdata" xml:lang="en">DAS</subject>
        <subject subjectScheme="SeisData" schemeURI="https://geofon.gfz-potsdam.de/cv/seisdata" xml:lang="en">MiniSEED</subject>
        <subject>Distributed dynamic strain sensing</subject>
        <subject>Distributed Acoustic Sensing</subject>
        <subject>DAS</subject>
        <subject subjectScheme="NASA/GCMD Earth Science Keywords">EARTH SCIENCE &gt; SOLID EARTH &gt; TECTONICS &gt; EARTHQUAKES &gt; SEISMIC PROFILE &gt; SEISMIC BODY WAVES</subject>
        <subject subjectScheme="NASA/GCMD Earth Science Keywords">EARTH SCIENCE &gt; SOLID EARTH &gt; TECTONICS &gt; EARTHQUAKES &gt; SEISMIC PROFILE &gt; SEISMIC SURFACE WAVES</subject>
    </subjects>
    <contributors>
        {% for creator in creators %}
        <contributor contributorType="ProjectManager">
            <contributorName nameType="Personal">{{ creator.name }}</contributorName>
            <givenName>COMPLETE!</givenName>
            <familyName>COMPLETE!</familyName>
            <nameIdentifier nameIdentifierScheme="ORCID">COMPLETE!</nameIdentifier>
            <!-- <affiliation>GFZ German Research Centre for Geosciences, Potsdam, Germany</affiliation> -->
        </contributor>
        {% endfor %}
    </contributors>
    <dates>
        <date dateType="Created">COMPLETE!</date>
        <date dateType="Collected">{{ starttime }}/{{ endtime }}</date>
    </dates>
    <resourceType resourceTypeGeneral="Dataset">Seismic Network</resourceType>
    <relatedIdentifiers>
        <relatedIdentifier relatedIdentifierType="URL" relationType="References">LINK TO RAW DATA!</relatedIdentifier>
        <relatedIdentifier relatedIdentifierType="URL" relationType="HasMetadata">LINK TO RAW METADATA!</relatedIdentifier>
    </relatedIdentifiers>
    <sizes>
        <size>COMPLETE!</size>
    </sizes>
    <formats/>
    <rightsList>
        <!-- <rights rightsIdentifier="CC-BY-4.0" rightsIdentifierScheme="SPDX" rightsURI="http://creativecommons.org/licenses/by/4.0/legalcode" schemeURI="https://spdx.org/licenses/">Creative Commons Attribution BY 4.0 International</rights> -->
    </rightsList>
    <descriptions>
        <description descriptionType="Abstract">{{ title }}</description>
    </descriptions>
    <geoLocations>
        <geoLocation>
            <geoLocationPlace>{{ geolocation.place }}</geoLocationPlace>
            <geoLocationBox>
                <westBoundLongitude>{{ geolocation.west }}</westBoundLongitude>
                <eastBoundLongitude>{{ geolocation.east }}</eastBoundLongitude>
                <southBoundLatitude>{{ geolocation.south }}</southBoundLatitude>
                <northBoundLatitude>{{ geolocation.north }}</northBoundLatitude>
            </geoLocationBox>
        </geoLocation>
    </geoLocations>
    <fundingReferences>
        <fundingReference>
            <funderName>{{ fundingagency }}</funderName>
            <!-- <funderIdentifier funderIdentifierType="Crossref Funder ID">http://doi.org/10.13039/501100010956</funderIdentifier> -->
        </fundingReference>
    </fundingReferences>
</resource>
