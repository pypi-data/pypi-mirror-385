vocabulary {
    type Node := {a, b, c}
    neighbour: Node * Node -> Bool
    reachable: Node * Node -> Bool
}

theory {
    {
        !n1, n2 in Node: reachable(n1, n2) <- neighbour(n1, n2).
        !n1, n2 in Node: reachable(n1, n2) <- ?n3 in Node: neighbour(n1, n3) & reachable(n3, n2).
    }
}

structure {
}
