    type Node <: Int
    edge: Node * Node -> Bool
    reachable: Node * Node -> Bool
}

theory T:V {
    {
        !x, z in Node: reachable(x, z) <- edge(x, z) | ?y in Node: edge(x, y) & reachable(y, z).
    }
}

