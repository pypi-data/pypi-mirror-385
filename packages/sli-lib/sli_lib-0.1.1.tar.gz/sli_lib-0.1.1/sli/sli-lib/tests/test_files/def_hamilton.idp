vocabulary {
    type lipLen := {Short, Standard}
    type jaDes := {Design_230, Design_400HP}
    LipLen: () -> lipLen
    JacketDesign: () -> jaDes
}

theory T:V {
    {
        !x in lipLen: LipLen() = x <- x = Short & JacketDesign() = Design_230.
        !x in lipLen: LipLen() = x <- x = Standard & JacketDesign() = Design_400HP.
    }
}

