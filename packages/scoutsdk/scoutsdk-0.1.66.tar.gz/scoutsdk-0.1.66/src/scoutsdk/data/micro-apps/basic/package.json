{
  "dependencies": {
    "@mirego/scout-api": "^1.0.23",
    "@mirego/scout-chat": "^1.0.176",
    "axios": "^1.8.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/parser": "7.4.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "5.4.3",
    "vite": "^6.2.0"
  },
  "name": "scout-micro-app",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "bun run format-write && bun run lint-write",
    "format-check": "bunx prettier --check ./src",
    "format-write": "bunx prettier --write ./src",
    "lint": "bun run type-check && bun run lint-check && bun run format-check",
    "lint-check": "bun run eslint --ext .js,.ts,.jsx,.tsx,.json .",
    "lint-write": "bun run eslint --ext .js,.ts,.jsx,.tsx,.json . --fix --quiet",
    "preview": "vite preview",
    "type-check": "bun run --bun tsc --noEmit"
  },
  "type": "module",
  "version": "0.0.0"
}
